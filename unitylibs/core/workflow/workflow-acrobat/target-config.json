{
  "_defaults": {
    "renderWidget": false,
    "uploadLimits": {
      "HIGH_END": { "files": 10, "chunks": 10 },
      "MID_RANGE": { "files": 5, "chunks": 10 },
      "LOW_END": { "files": 3, "chunks": 6 }
    },
    "sendSplunkAnalytics": true,
    "fetchApiConfig": {
      "finalizeAsset": {
        "retryType": "polling",
        "retryParams": {
          "maxRetryDelay": 300000,
          "defaultRetryDelay": 5000
        }
      },
      "getMetadata": {
        "retryType": "polling",
        "retryParams": {
          "maxRetryDelay": 5000,
          "defaultRetryDelay": 500
        }
      },
      "default": {
        "retryType": "exponential",
        "retryParams": {
          "maxRetries": 4,
          "retryDelay": 1000
        }
      }
    }
  },
  "verb-widget": {
    "selector": ".verb-wrapper",
    "source": ".verb-wrapper .verb-container",
    "target": ".verb-wrapper .verb-container",
    "showSplashScreen": true,
    "splashScreenConfig": {
      "fragmentLink": "/dc-shared/fragments/shared-fragments/frictionless/splash-page/splashscreen",
      "splashScreenParent": "body"
    },
    "actionMap": {
      ".verb-wrapper": "upload",
      "#file-upload": "upload"
    },
    "verbsWithoutMfuToSfuFallback": ["compress-pdf"],
    "nonpdfMfuFeedbackScreenTypeNonpdf": ["combine-pdf"],
    "nonpdfSfuProductScreen": ["word-to-pdf", "jpg-to-pdf", "ppt-to-pdf", "excel-to-pdf", "png-to-pdf", "createpdf", "chat-pdf", "chat-pdf-student", "summarize-pdf", "pdf-ai"],
    "mfuUploadAllowed": ["combine-pdf", "rotate-pages", "chat-pdf", "chat-pdf-student", "summarize-pdf", "pdf-ai"],
    "mfuUploadOnlyPdfAllowed": ["combine-pdf"],
    "experimentationOn": ["add-comment"]
  },
  "study-marquee": {
    "selector": ".foreground",
    "source": ".foreground .study-marquee-container",
    "target": ".foreground .study-marquee-container",
    "showSplashScreen": true,
    "splashScreenConfig": {
      "fragmentLink": "/dc-shared/fragments/shared-fragments/frictionless/splash-page/splashscreen",
      "splashScreenParent": "body"
    },
    "actionMap": {
      ".foreground": "upload",
      "#file-upload": "upload"
    },
    "nonpdfSfuProductScreen": ["quiz-maker", "flashcard-maker"],
    "mfuUploadAllowed": ["quiz-maker", "flashcard-maker"]
  }
}
