{
  "acom-widget": {
    "type": "pdf",
    "selector": ".acom-wrapper",
    "handler": "render",
    "renderWidget": false,
    "source": ".acom-wrapper .acom-container",
    "target": ".acom-wrapper .acom-container",
    "actionMap": {
      ".acom-wrapper": [
        {
          "actionType": "drop"
        }
      ],
      "#file-upload": [
        {
          "actionType": "upload"
        }
      ]
    }
  },
  "marquee": {
    "type": "pdf",
    "selector": ".action-area",
    "handler": "render",
    "renderWidget": false,
    "source": ".action-area .con-button",
    "target": ".action-area .con-button",
    "actionMap": {
      ".action-area .con-button": [
        {
          "actionType": "upload"
        }
      ]
    }
  }
}
