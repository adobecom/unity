{
  "hero-marquee": {
    "type": "text",
    "selector": ".copy",
    "handler": "render",
    "renderWidget": true,
    "useFireflyPromptBar": true,
    "promptBarEnvironment": {
      "type": "stage",
      "localeCode": "en-US"
    },
    "promptBarSettingsConfig": {
      "hideMoreButton": false
    },
    "source": ".copy .main-copy",
    "target": ".row-supplemental a",
    "insert": "before",
    "verbsWithoutPromptSuggestions": ["vector"],
    "actionMap": {
      ".inp-field": { "actionType": "autocomplete" },
      ".gen-btn": { "actionType": "generate" },
      ".drop-item": { "actionType": "setPromptValue" }
    },
    "sendSplunkAnalytics": true
  },
  "showcase-marquee": {
    "type": "text",
    "selector": ".text",
    "handler": "render",
    "renderWidget": true,
    "useFireflyPromptBar": true,
    "promptBarEnvironment": {
      "type": "stage",
      "localeCode": "en-US"
    },
    "promptBarSettingsConfig": {
      "hideMoreButton": false
    },
    "source": ".text",
    "target": "p:last-of-type",
    "insert": "after",
    "verbsWithoutPromptSuggestions": ["vector"],
    "actionMap": {
      ".inp-field": { "actionType": "autocomplete" },
      ".gen-btn": { "actionType": "generate" },
      ".drop-item": { "actionType": "setPromptValue" }
    },
    "sendSplunkAnalytics": true
  }
}
