{
  "hero-marquee": {
    "type": "text",
    "selector": ".copy",
    "handler": "render",
    "renderWidget": true,
    "source": ".copy .main-copy",
    "target": "a:last-of-type",
    "insert": "before",
    "verbsWithoutPromptSuggestions": ["vector"],
    "actionMap": {
      ".inp-field": { "actionType": "autocomplete" },
      ".gen-btn": { "actionType": "generate" },
      ".drop-item": { "actionType": "setPromptValue" }
    },
    "sendSplunkAnalytics": true
  },
    "showcase-marquee": {
    "type": "text",
    "selector": ".text",
    "handler": "render",
    "renderWidget": true,
    "source": ".text",
    "target": "p:last-of-type",
    "insert": "after",
    "verbsWithoutPromptSuggestions": ["vector"],
    "actionMap": {
      ".inp-field": { "actionType": "autocomplete" },
      ".gen-btn": { "actionType": "generate" },
      ".drop-item": { "actionType": "setPromptValue" }
    },
    "sendSplunkAnalytics": true
  }
}
