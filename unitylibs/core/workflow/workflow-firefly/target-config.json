{
  "hero-marquee": {
    "type": "text",
    "selector": ".copy",
    "handler": "render",
    "renderWidget": true,
    "source": ".copy .main-copy",
    "target": "p:last-of-type",
    "insert": "before",
    "floatPrompt": false,
    "actionMap": {
      ".inp-field": [
        {
          "actionType": "autocomplete"
        }
      ],
      ".gen-btn": [
        {
          "actionType": "generate"
        }
      ],
      ".refresh-btn": [
        {
          "actionType": "refreshSuggestion"
        }
      ],
      ".close-btn": [
        {
          "actionType": "closeDropdown"
        }
      ]
    }
  }
}
